Table dim_paciente {
  id_paciente int [pk]
  nombre varchar
  apellido varchar
  fecha_nac date
}

Table dim_medico {
  id_medico int [pk]
  nombre varchar
  apellido varchar
  matricula varchar
  especialidad varchar
}

Table dim_estado {
  id_estado int [pk]
  nombre varchar
}

Table dim_sede {
  id_sede int [pk]
  nombre varchar
  direccion varchar
}

Table dim_fecha {
  id_fecha int [pk]
  dia int
  mes int
  anio int
}

Table fact_turno {
  id_turno int [pk]
  id_paciente int [ref: > dim_paciente.id_paciente]
  id_medico int [ref: > dim_medico.id_medico]
  id_estado int [ref: > dim_estado.id_estado]
  id_sede int [ref: > dim_sede.id_sede]
  id_fecha int [ref: > dim_fecha.id_fecha]
  motivo varchar
}


Ref: "dim_estado"."id_estado" < "dim_estado"."nombre"